#!/usr/bin/make -f

export PYBUILD_NAME=python3-django-horizon-cloudveneto
%:
	dh $@ --with python3 --buildsystem=pybuild

override_dh_auto_install:
	dh_auto_install
	mkdir -p $(CURDIR)/debian/python3-django-horizon-cloudveneto/etc
	install -m 755 $(CURDIR)/config/notifications_en.txt $(CURDIR)/debian/python3-django-horizon-cloudveneto/etc

